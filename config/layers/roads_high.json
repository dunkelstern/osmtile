{
    "name": "Roads",
    "db_table": "osm_roads",
    "type": "line",
    "text_column": "name",
    "style": "road_residential",
    "width": 7,
    "min_pixel_width": 2,
    "stroke": 2,
    "filter": {
        "type": [
            { "=": "secondary" },
            { "=": "secondary_link" },
            { "=": "tertiary" },
            { "=": "tertiary_link" },
            { "=": "unclassified" },
            { "=": "residential" },
            { "=": "living_street" }
        ],
        "class": [
            { "=": "highway" }
        ]
    }
}
